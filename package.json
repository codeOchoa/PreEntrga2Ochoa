{
    "title": "Malec",
    "name": "malec-homepage",
    "version": "0.0.1",
    "scripts": {
        "build": "npm run clean && npm run build:scss && npm run build:scripts && npm run build:assets",
        "build:assets": "node scripts/build-assets.js",
        "build-css": "sass styles/main.scss assets/css/main.css",
        "build:scripts": "node scripts/build-scripts.js",
        "build:scss": "node scripts/build-scss.js",
        "clean": "node scripts/clean.js",
        "start": "npm run build && node scripts/start.js",
        "start:debug": "npm run build && node scripts/start-debug.js",
        "watch-css": "sass --watch styles/main.scss:assets/css/main.css"
    },
    "description": "A shop homepage HTML template built with Bootstrap",
    "keywords": [
        "css",
        "sass",
        "html",
        "responsive",
        "theme",
        "template"
    ],
    "homepage": "",
    "bugs": {
        "url": "https://github.com/codeOchoa/PreEntrga2Ochoa/issues",
        "email": "codeOchoa@gmail.com"
    },
    "license": "",
    "author": "Start Bootstrap",
    "contributors": [
        "David Miller (https://davidmiller.io/)"
    ],
    "repository": {
        "type": "git",
        "url": "https://github.com/codeOchoa/PreEntrga2Ochoa"
    },
    "dependencies": {
        "bootstrap": "^5.3.3",
        "bootstrap-icons": "^1.11.3"
    },
    "devDependencies": {
        "sass": "^1.77.8"
    }
}